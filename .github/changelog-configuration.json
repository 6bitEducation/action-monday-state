{
  "categories": [
    {
      "title": "## üöÄ Features",
      "labels": ["feature"]
    },
    {
      "title": "## üêõ Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## üß™ Tests",
      "labels": ["test"]
    },
    {
      "title": "## \uD83D\uDCDC Docs",
      "labels": ["docs"]
    }
  ],
  "label_extractor": [
    {
      "pattern": "^feature",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^fix",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^test",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^docs",
      "method": "match",
      "on_property": "title"
    }
  ],
  "sort": "DESC",
  "template": "${{CHANGELOG}}\n\n## \uD83D\uDCDC Uncategorized:\n\n${{UNCATEGORIZED}}\n",
  "pr_template": "- ${{TITLE}} \n  - Author: ${{AUTHOR}}\n  - Reviewers: ${{REVIEWERS}}",
  "empty_template": "- No Changes detected"
}
